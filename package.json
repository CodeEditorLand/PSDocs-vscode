{
	"name": "psdocs-vscode",
	"displayName": "PSDocs.Azure (Preview)",
	"version": "0.0.1",
	"private": false,
	"description": "Generate Markdown from ARM templates using PSDocs.Azure",
	"keywords": [
		"land"
	],
	"homepage": "HTTPS://GitHub.Com/CodeEditorLand/PSDocsLand#readme",
	"bugs": {
		"url": "HTTPS://GitHub.Com/CodeEditorLand/PSDocsLand/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+HTTPS://github.com/CodeEditorLand/PSDocsLand.git"
	},
	"license": "SEE LICENSE IN LICENSE",
	"author": {
		"name": "üõ¨‚ÄÅLand‚ÄÅ‚Äî",
		"email": "Land@PlayForm.Cloud",
		"url": "HTTPS://Land.PlayForm.Cloud"
	},
	"type": "module",
	"main": "./out/extension.js",
	"scripts": {
		"Document": "Document 'Source/**/*.ts'",
		"compile": "tsc -p ./",
		"esbuild": "npm run -S esbuild-base -- --sourcemap",
		"esbuild-base": "esbuild ./src/extension.ts --bundle --outfile=out/extension.js --external:vscode --format=cjs --platform=node",
		"esbuild-watch": "npm run -S esbuild-base -- --sourcemap --watch",
		"pack": "vsce package",
		"prepublishOnly": "Build 'Source/**/*.ts'",
		"publish": "vsce publish"
	},
	"contributes": {
		"commands": [
			{
				"command": "vicperdana.psdocs-vscode-preview",
				"title": "PSDocs.Azure: Generate Markdown"
			}
		],
		"snippets": [
			{
				"language": "arm-template",
				"path": "./snippets/arm.json"
			}
		]
	},
	"activationEvents": [
		"onCommand:vicperdana.psdocs-vscode-preview"
	],
	"dependencies": {},
	"devDependencies": {
		"@playform/build": "0.0.7",
		"@playform/document": "0.0.6",
		"@types/glob": "8.1.0",
		"@types/node": "20.12.10",
		"esbuild": "0.20.2",
		"glob": "10.3.12"
	},
	"extensionDependencies": [
		"ms-vscode.powershell",
		"msazurermtools.azurerm-vscode-tools",
		"vscode.powershell"
	],
	"publishConfig": {
		"access": "public"
	},
	"icon": "images/psdocs-icon.png",
	"galleryBanner": {
		"color": "#0072c6",
		"theme": "dark"
	},
	"preview": true
}
